    border-bottom-width: 16px;
    border-left-width: 16px;
    border-top-left-radius: 12px;
    border-top-right-radius: 12px;
    border-bottom-right-radius: 12px;
    border-bottom-left-radius: 12px;
    font-size: 28px;
    -unity-font-definition: url("project://database/Assets/Fonts/alagard.ttf?fileID=12800000&guid=ae5e69613b5fc6c4ba3abfa6c8ac0ca2&type=3#alagard");
    -unity-text-outline-color: rgb(125, 225, 214);
